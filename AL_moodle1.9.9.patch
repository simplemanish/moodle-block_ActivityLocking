+                    $attempt = scorm_get_last_attempt($sco->scorm,$USER->id);
+
+
+                    $track=scorm_get_tracks($sco->id,$USER->id,$attempt);
+
+
+
+
+                    list($uspendhour, $uspendminute,$uspendsec) = explode(":", $track->total_time);
