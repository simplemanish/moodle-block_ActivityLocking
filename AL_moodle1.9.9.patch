+                $row[4] = (isset($lock['T']["usertime"]) && $lock['T']["usertime"] >= $lock['T']["mintime"] ) ? '<div class="highlight">'.$usertimevalue.'</div>' : '<div style="background:red">'.$usertimevalue.'</div>';
